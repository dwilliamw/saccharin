<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>visionsugar.tk</title>
    <link rel="icon" href="img/exclamation-mark.png" />
    <!-- password js -->
    <script src="~/Scripts/password.js"></script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <!-- animate CSS -->
    <link rel="stylesheet" href="~/Content/animate.css" />
    <!-- owl carousel CSS -->
    <link rel="stylesheet" href="~/Content/owl.carousel.min.css" />
    <!-- themify CSS -->
    <link rel="stylesheet" href="~/Content/themify-icons.css" />
    <!-- flaticon CSS -->
    <link rel="stylesheet" href="~/Content/flaticon.css" />
    <!-- font awesome CSS -->
    <link rel="stylesheet" href="~/Content/magnific-popup.css" />
    <!-- swiper CSS -->
    <link rel="stylesheet" href="~/Content/slick.css" />
    <link rel="stylesheet" href="~/Content/gijgo.min.css" />
    <link rel="stylesheet" href="~/Content/nice-select.css" />
    <link rel="stylesheet" href="~/Content/all.css" />
    <!-- style CSS -->
    <link rel="stylesheet" href="~/Content/style.css" />
    <!-- To the top CSS-->
    <link rel="stylesheet" href="~/Content/tothetop.css">
</head>
<body>
    <!--::header part start::-->
    <header class="main_menu home_menu">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a class="nav-link" href="@Url.Action("Index","Home")">
                            <img src="~/img/eyeglasses.svg" alt="logo" height="40" width="40" />
                            <img src="~/img/sugar.svg" alt="logo" height="40" width="40" />
                        </a>
                        <button class="navbar-toggler"
                                type="button"
                                data-toggle="collapse"
                                data-target="#navbarSupportedContent"
                                aria-controls="navbarSupportedContent"
                                aria-expanded="false"
                                aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse main-menu-item justify-content-end"
                             id="navbarSupportedContent">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" @Html.ActionLink("Home", "Index")</a>
                                </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle"
                                       id="navbarDropdown"
                                       role="button"
                                       data-toggle="dropdown"
                                       aria-haspopup="true"
                                       aria-expanded="false">
                                        Sugar World
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                        <a class="dropdown-item" @Html.ActionLink("What is Sugar?", "Whatissugar")</a>
                                        <a class="dropdown-item" @Html.ActionLink("Types of Sugar", "Sugartype")</a>
                                    </div>
                                </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle"
                                       id="navbarDropdwon"
                                       role="button"
                                       data-toggle="dropdown"
                                       aria-haspopup="true"
                                       aria-expanded="false">
                                        Analysis Tools
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                        <a class="dropdown-item" @Html.ActionLink("Sugar Analysis", "SugarAnalysis")</a>
                                        <a class="dropdown-item" @Html.ActionLink("Check Your BMI", "Bmi")</a>
                                        <a class="dropdown-item" @Html.ActionLink("Check Your Stress Level", "Stress")</a>
                                    </div>
                                </li>
                                <li>
                                    <a class="nav-link" @Html.ActionLink("Burn Your Calories", "Exercise")</a>
                                </li>
                                @*<li class="nav-item">
                                    <a class="nav-link" @Html.ActionLink("Alternative Foods", "Alternative")</a>
                                </li>*@
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle"
                                       id="navbarDropdwon"
                                       role="button"
                                       data-toggle="dropdown"
                                       aria-haspopup="true"
                                       aria-expanded="false">
                                        Useful Advice
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                        <a class="dropdown-item" @Html.ActionLink("Alternative Foods", "Alternative")</a>
                                        <a class="dropdown-item" @Html.ActionLink("Stress Management", "stressmanagement")</a>
                                    </div>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- Header part end-->
    <div class="container-fluid body-content"> </div>
    <!--up to top button start-->
    <button onclick="topFunction()" id="myBtm" title="Go to top">▲</button>
    <!--up to top button end-->
    @RenderBody()
    <hr/>
    <!-- footer part start-->
    <footer class="footer-area">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-xl-3 col-sm-6 col-md-3 col-lg-3">
                    <div class="single-footer-widget footer_1">
                        <a class="nav-link" href="@Url.Action("Index","Home")">
                            <img src="~/img/eyeglasses.svg" alt="logo" height="40" width="40" />
                            <img src="~/img/sugar.svg" alt="logo" height="40" width="40" />
                        </a>
                        <br><span>Our Aims: The Dangers of Excessive Sugar Intake Should Be Noted</span><br />
                    </div>
                </div>
                <div class="col-xl-3 col-sm-6 col-md-2 col-lg-3">
                        <div class="single-footer-widget footer_2">
                            <h4>Quick links</h4>
                            <div class="contact_info">
                                <ul>
                                    <li>@Html.ActionLink("Home", "Index")</li>
                                    <li>@Html.ActionLink("What Is Sugar?", "Whatissugar")</li>
                                    <li>@Html.ActionLink("Types of Sugar", "Sugartype")</li>
                                    <li>@Html.ActionLink("Sugar Analysis", "SugarAnalysis")</li>
                                    <li>@Html.ActionLink("Check Your BMI", "Bmi")</li>
                                    <li>@Html.ActionLink("Check Your Stress Level", "Stress")</li>
                                    <li>@Html.ActionLink("Burn Your Calories", "Exercise")</li>
                                    <li>@Html.ActionLink("Alternative Foods", "Alternative")</li>
                                    <li>@Html.ActionLink("Stress Advice", "stressmanagement")</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                <div class="col-xl-3 col-sm-6 col-md-4 col-lg-3">
                    <div class="single-footer-widget footer_3">
                        <h4>Contact Us</h4>
                        <form action="#">
                            <div class="form-group">
                            </div>
                        </form>
                        <p>
                            Feel Free to Leave a Message!
                        </p><br />
                        <p>    
                        Click This: @Html.ActionLink("Contact Us", "Contact")
                        </p>
                    </div>
                    </div>
            </div>
            <div class="copyright_part_text">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <p class="footer-text m-0">
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="ti-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer part end-->
    <!-- jquery plugins here-->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <!-- popper js -->
    <script src="~/Scripts/popper.min.js"></script>
    <!-- bootstrap js -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- easing js -->
    <script src="~/Scripts/jquery.magnific-popup.js"></script>
    <!-- swiper js -->
    <script src="~/Scripts/swiper.min.js"></script>
    <!-- swiper js -->
    <script src="~/Scripts/masonry.pkgd.js"></script>
    <!-- particles js -->
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <!-- swiper js -->
    <script src="~/Scripts/slick.min.js"></script>
    <script src="~/Scripts/gijgo.min.js"></script>
    <script src="~/Scripts/jquery.nice-select.min.js"></script>
    <!-- custom js -->
    <script src="~/Scripts/custom.js"></script>
    <!-- to the top js-->
    <script src="~/Scripts/tothetop.js"></script>
</body>
</html>
